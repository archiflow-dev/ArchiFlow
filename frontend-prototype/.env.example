# ArchiFlow Frontend Environment Variables
# Copy this file to .env and configure for your environment

# Backend API URL for REST API
# IMPORTANT: Must include trailing slash!
# The URL constructor requires the trailing slash to properly concatenate paths.
VITE_API_URL=http://localhost:8000/api/

# WebSocket URL for Socket.IO connection
# No /api prefix - Socket.IO is mounted at root
VITE_WS_URL=http://localhost:8000
